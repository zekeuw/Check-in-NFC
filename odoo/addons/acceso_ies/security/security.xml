<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record model="ir.module.category" id="module_category_acceso_ies">
        <field name="name">Control Accesos IES</field>
        <field name="description">Gesti√≥n de accesos de alumnos y profesores</field>
        <field name="sequence">10</field>
    </record>

    <record id="group_acceso_ies_user" model="res.groups">
        <field name="name">Usuario</field>
        <field name="category_id" ref="module_category_acceso_ies"/>
    </record>

    <record id="group_acceso_ies_manager" model="res.groups">
        <field name="name">Administrador</field>
        <field name="category_id" ref="module_category_acceso_ies"/>
        <field name="implied_ids" eval="[(4, ref('group_acceso_ies_user'))]"/>
        <field name="users" eval="[(4, ref('base.user_root')), (4, ref('base.user_admin'))]"/>
    </record>

</odoo>